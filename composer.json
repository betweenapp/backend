{
    "name": "betweenapp/backend",
    "description": "The backend package for the Between App",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Barrento",
            "email": "francisco.barrento@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url" : "https://github.com/betweenapp/core"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "betweenapp/core": "*"
    },
    "require-dev": {
        "mockery/mockery": "*",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "orchestra/testbench-browser-kit": "^4.0",
        "php-coveralls/php-coveralls": "^2.1",
        "orchestra/testbench-dusk": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Betweenapp\\Backend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Betweenapp\\Backend\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Betweenapp\\Backend\\BackendServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
