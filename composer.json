{
    "name": "betweenapp/backend",
    "description": "The backend package for the Between App",
    "license": "MIT",
    "authors": [
        {
            "name": "Francisco Barrento",
            "email": "francisco.barrento@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url" : "https://github.com/betweenapp/core"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "betweenapp/core": "*"
    },
    "autoload": {
        "psr-4": {
            "Betweenapp\\Backend\\": "src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Betweenapp\\Backend\\BackendServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
